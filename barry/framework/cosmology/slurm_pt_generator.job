#!/bin/bash -l
#SBATCH -p smp
#SBATCH -J ptgenerator
#SBATCH --mem-per-cpu=4GB
#SBATCH -t 04:00:00
#SBATCH -o zlog.o%j
#SBATCH -n 50

module load openmpi-x86_64
conda deactivate
conda activate {conda_env}
echo $PATH
echo "Activated python"
echo `which python`

/opt/openmpi/bin/mpirun python PT_generator.py